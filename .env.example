# API Keys
OPENAI_API_KEY=sk-your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here
GOOGLE_API_KEY=your-google-key-here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/reflexion
REDIS_URL=redis://localhost:6379/0

# Memory Configuration
MEMORY_BACKEND=postgresql  # Options: memory, postgresql, redis
EMBEDDING_MODEL=text-embedding-ada-002
MEMORY_RETENTION_DAYS=90

# Monitoring
ENABLE_TELEMETRY=true
LANG_OBSERVATORY_ENDPOINT=http://localhost:8080
LANG_OBSERVATORY_API_KEY=your-observatory-key

# Development
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=development

# Framework-specific settings
AUTOGEN_CONFIG_LIST_PATH=./config/autogen_models.json
CREWAI_ENABLE_TELEMETRY=false

# Security
SECRET_KEY=your-secret-key-for-encryption
ENABLE_AUTH=false
API_RATE_LIMIT=100

# Testing
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/reflexion_test
PYTEST_MARKERS=unit,integration,benchmark